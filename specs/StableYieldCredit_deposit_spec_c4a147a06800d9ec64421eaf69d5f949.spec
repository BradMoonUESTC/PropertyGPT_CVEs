function _deposit(IERC20,uint256) public  {}

rule VerifyLiquidityAndCreditAfterDeposit() {
    address $tokenAddr;
    uint256 $depositAmount;
    uint256 $initCollateral = collateral[msg.sender][$tokenAddr];
    uint256 $initCredit = collateralCredit[msg.sender][$tokenAddr];

    _deposit(IERC20($tokenAddr), $depositAmount);

    uint256 calculatedValue = LINK.getPriceUSD($tokenAddr) * $depositAmount / uint256(10)**IERC20($tokenAddr).decimals();
    
    assert(collateral[msg.sender][$tokenAddr] > $initCollateral);
    assert(collateralCredit[msg.sender][$tokenAddr] == $initCredit + calculatedValue);
}}